{
  "name": "github-detial-comment",
  "displayName": "github-detial-comment",
  "description": "",
  "version": "0.0.1",
  "publisher": "wk-j",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello",
    "onCommand:extension.githubDetailComment"
  ],
  "main": "./out/src/extension",
